@page
@model KT.Areas.Identity.Pages.Account.LoginModel
@{
    ViewData["Title"] = "Log in";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Input.MaSV"></label>
        <input asp-for="Input.MaSV" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Log in</button>
</form>
